{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "electron typescript template project",
  "main": "./app/main.js",
  "scripts": {
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "clean": "gulp clean",
    "lint": "eslint --ext .ts \"./**/*.ts\"",
    "format": "prettier --write \"./**/*.ts\" \"./prettier.config.js\" \"./**/*.html\" \"./**/*.css\" \"./{tests,specs}/*.json\"",
    "utest": "npm run build && nyc mocha --config ./tests/.mocharc.json ./dist/tests/tests.spec.js",
    "btest": "npm run build && nyc mocha --config ./specs/.mocharc.json ./dist/specs/tests.spec.js",
    "start": "npm run build && electron ./dist/src/app/main.js",
    "release": "gulp release"
  },
  "author": "MetaBarj0",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/cpx": "^1.5.0",
    "@types/electron": "^1.6.10",
    "@types/gulp": "^4.0.6",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/gulp-typescript": "^2.13.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@typescript-eslint/typescript-estree": "^1.9.0",
    "angular-html-parser": "^1.2.0",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "electron": "^5.0.2",
    "electron-packager": "^13.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-mocha": "^6.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
