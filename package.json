{
  "name": "template",
  "version": "1.0.0",
  "description": "electron typescript template project",
  "main": "./app/main.js",
  "scripts": {
    "build": "npx tsc && cpx \"./src/app/**/*.{html,css}\" ./dist/src/app",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .ts \"./**/*.ts\"",
    "format": "prettier --write \"./**/*.ts\" \"./prettier.config.js\" \"./**/*.html\" \"./**/*.css\"",
    "unit-tests": "npm run build && mocha --config ./tests/.mocharc.json ./dist/tests/tests.spec.js",
    "behavior-tests": "npm run build && mocha --config ./specs/.mocharc.json ./dist/specs/tests.spec.js",
    "test": "npm run build && mocha --config ./tests/.mocharc.json ./dist/tests/tests.spec.js && mocha --config ./specs/.mocharc.json ./dist/specs/tests.spec.js",
    "start": "npm run build && electron ./dist/src/app/main.js"
  },
  "author": "MetaBarj0",
  "license": "MIT",
  "dependencies": {
    "electron": "^5.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/electron": "^1.6.10",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@typescript-eslint/typescript-estree": "^1.9.0",
    "angular-html-parser": "^1.2.0",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
